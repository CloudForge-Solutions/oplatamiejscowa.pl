{
  "name": "oplata-miejscowa",
  "version": "1.0.0",
  "description": "Tourist tax payment system for Polish cities - pay directly online with lowest fees",
  "type": "module",
  "main": "src/shell/main.tsx",
  "scripts": {
    "dev": "npm run 'dev:check' && vite --port 3040 --host 0.0.0.0",
    "dev:react": "vite --port 3040 --host 0.0.0.0",
    "dev:check": "npx tsc --noEmit && npx eslint src --ext .ts,.tsx",
    "build": "vite build",
    "preview": "vite preview --port 3041",
    "dev:quality": "concurrently \"npm run lint:watch\" \"npm run test:watch\" --names \"LINT,TEST\" --prefix-colors \"yellow,green\"",
    "dev:scss": "sass --watch src/styles:dist/styles --style=expanded",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:watch": "nodemon --watch src --ext js,jsx,ts,tsx --exec \"npm run lint\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "quality:check": "concurrently \"npm run lint\" \"npm run format:check\" \"npm run test\" --names \"LINT,FORMAT,TEST\" --prefix-colors \"yellow,blue,green\"",
    "jscpd": "jscpd src --min-lines 5 --min-tokens 50 --reporters html,console --output reports/duplication",
    "jsinspect": "jsinspect src --threshold 30 --ignore test > reports/jsinspect.json"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vitejs/plugin-react": "^4.7.0",
    "axios": "^1.6.0",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.13.1",
    "eslint-plugin-react": "^7.37.5",
    "graphql": "^16.11.0",
    "graphql-request": "^6.1.0",
    "html2canvas": "^1.4.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jspdf": "^2.5.2",
    "openai": "^4.67.0",
    "pdfjs-dist": "^4.0.0",
    "puppeteer-core": "^21.0.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.1",
    "react-router-bootstrap": "^0.26.3",
    "react-router-dom": "^7.7.0",
    "serialize-query-params": "^2.0.2",
    "tesseract.js": "^5.0.0",
    "use-query-params": "^2.2.1",
    "uuid": "^11.0.0",
    "weaviate-ts-client": "^2.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "*",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.40.0",
    "@stylistic/eslint-plugin": "^5.2.0",
    "@types/node": "^22.17.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "chokidar": "^3.5.0",
    "chokidar-cli": "^3.0.0",
    "chromedriver": "^138.0.2",
    "concurrently": "^8.2.0",
    "cypress": "^13.0.0",
    "eslint": "*",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "handlebars": "^4.7.8",
    "http-server": "^14.0.0",
    "i18n-lint": "^1.1.0",
    "jscpd": "^3.5.0",
    "jsdoc": "^4.0.0",
    "jsdom": "^23.0.0",
    "jsinspect": "^0.12.7",
    "lighthouse": "^11.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "sass": "^1.69.0",
    "selenium-webdriver": "^4.34.0",
    "stylelint": "^16.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "terser": "^5.24.0",
    "typescript": "^5.8.3",
    "vite": "^5.4.0",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "tourist-tax",
    "oplata-miejscowa",
    "poland",
    "payment",
    "imoje",
    "react",
    "typescript",
    "spa",
    "indexeddb",
    "gdpr-compliant"
  ],
  "author": "Robert Grzelka",
  "license": "MIT"
}
